{
    "version": 2,
    "builds": [
        {
            "src": "api/*.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/wa",
            "dest": "/api/wa.js"
        },
        {
            "src": "/tg",
            "dest": "/api/tg.js"
        }
    ]
}