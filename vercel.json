{
  "version": 2,
  "builds": [
    {
      "src": "api/**/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/telegram",
      "dest": "/api/tg.js"
    },
    {
      "src": "/api/whatsapp",
      "dest": "/api/wa.js"
    },
    {
      "src": "/api/debug",
      "dest": "/api/debug.js"
    },
    {
      "src": "/api/setup",
      "dest": "/api/setup.js"
    },
    {
      "src": "/api/warmup",
      "dest": "/api/warmup.js"
    },
    {
      "src": "/(.*)",
      "dest": "/api/index.js"
    }
  ]
}
