{
  "version": 2,
  "builds": [
    {
      "src": "api/**/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/telegram/webhook",
      "dest": "/api/telegram.js"
    },
    {
      "src": "/api/whatsapp/webhook",
      "dest": "/api/whatsapp.js"
    },
    {
      "src": "/(.*)",
      "dest": "/api/index.js"
    }
  ]
}
