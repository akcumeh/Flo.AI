{
  "version": 2,
  "builds": [
    {
      "src": "api/**/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/tg",
      "dest": "/api/tg.js"
    },
    {
      "src": "/api/wa",
      "dest": "/api/wa.js"
    },
    {
      "src": "/api/debug",
      "dest": "/api/debug.js"
    },
    {
      "src": "/api/setup",
      "dest": "/api/setup.js"
    },
    {
      "src": "/api/warmup",
      "dest": "/api/warmup.js"
    },
    {
      "src": "/api/cron/streak-reminders",
      "dest": "/api/cron/streak-reminders.js"
    },
    {
      "src": "/api/cron/streak-resets",
      "dest": "/api/cron/streak-resets.js"
    },
    {
      "src": "/(.*)",
      "dest": "/api/index.js"
    }
  ],
  "crons": [
    {
      "path": "/api/cron/streak-reminders",
      "schedule": "0 22 * * *"
    },
    {
      "path": "/api/cron/streak-resets",
      "schedule": "1 0 * * *"
    }
  ]
}
